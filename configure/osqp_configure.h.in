#ifndef OSQP_CONFIGURE_H
# define OSQP_CONFIGURE_H


/* OSQP_ENABLE_DEBUG */
#cmakedefine OSQP_ENABLE_DEBUG

/* Operating system */
#cmakedefine IS_LINUX
#cmakedefine IS_MAC
#cmakedefine IS_WINDOWS

/* Algebra backend in use - Only one of the following is defined at compilation time */
#cmakedefine OSQP_ALGEBRA_BUILTIN
#cmakedefine OSQP_ALGEBRA_MKL
#cmakedefine OSQP_ALGEBRA_CUDA
#cmakedefine OSQP_ALGEBRA_OPENCL

/* Enable code generation */
#cmakedefine OSQP_CODEGEN

/* Enable profiler annotations */
#cmakedefine OSQP_PROFILER_ANNOTATIONS

/* Enable derivative computation in the solver */
#cmakedefine OSQP_ENABLE_DERIVATIVES

/* OSQP_EMBEDDED_MODE */
#cmakedefine OSQP_EMBEDDED_MODE (@OSQP_EMBEDDED_MODE@)

/* Header file containing custom memory allocators */
#cmakedefine OSQP_CUSTOM_MEMORY "@OSQP_CUSTOM_MEMORY@"

/* OSQP_ENABLE_PRINTING */
#cmakedefine OSQP_ENABLE_PRINTING

/* Header file containing custom printing functions */
#cmakedefine OSQP_CUSTOM_PRINTING "@OSQP_CUSTOM_PRINTING@"

/* OSQP_ENABLE_PROFILING */
#cmakedefine OSQP_ENABLE_PROFILING

/* OSQP_ENABLE_INTERRUPT */
#cmakedefine OSQP_ENABLE_INTERRUPT

/* OSQP_USE_FLOAT */
#cmakedefine OSQP_USE_FLOAT

/* OSQP_USE_LONG */
#cmakedefine OSQP_USE_LONG

#endif /* ifndef OSQP_CONFIGURE_H */
